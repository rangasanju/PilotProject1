<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bujo.pilotproject">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/bgimg"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomePageActivity" />
        <activity android:name=".FillBiodata" />
        <activity android:name=".ViewBiodata" />
        <activity android:name=".PersonalDetailsForm" />
        <activity android:name=".FamilyDetailsForm" />
        <activity android:name=".UploadDocumentsForm" />
        <activity android:name=".Physical_CheckupForm"></activity>
    </application>

</manifest>